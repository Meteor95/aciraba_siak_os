{
    "name": "aciraba/aciraba-1.0.0",
    "description": "ACIRABA - MAKE YOUR BUSSINES FUN",
    "license": "MIT",
    "type": "project",
    "homepage": "https://erayadigital.co.id",
    "support": {
        "forum": "https://forum.eraya.com/",
        "source": "https://github.com/codeigniter4/CodeIgniter4"
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "codeigniter4/framework": "^4.0",
        "mike42/escpos-php": "^4.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
